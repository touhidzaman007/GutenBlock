{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/guten-block",
  "version": "0.1.0",
  "title": "Guten Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "newData": {
      "type": "object",
      "default": null
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "textdomain": "guten-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}